{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "react/jsx-filename-extension": "off",
    "react/require-default-props": "off",
    "no-underscore-dangle": "off",
    "no-use-before-define": "off",
    "no-shadow": "off",
    "jsx-a11y/label-has-for": "off",
    "no-alert": "off"
  }
}
